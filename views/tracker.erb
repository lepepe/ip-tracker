<div class="inner cover">
  <table class="table table-hover">
    <thead>
      <tr>
        <th colspan="2">Lookup IP Address Location For IP: <%= @location[0].data["ip"] %></th>
      </tr>
    </thead>
    <tbody>
      <% @location[0].data.each do |key, value| %>
      <tr>
        <td><%= key %></td>
        <td><%= value %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
